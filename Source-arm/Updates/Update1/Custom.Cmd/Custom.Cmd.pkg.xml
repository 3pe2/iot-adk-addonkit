<?xml version="1.0" encoding="utf-8"?>
 <Package xmlns="urn:Microsoft.WindowsPhone/PackageSchema.v8.00"
    Owner="$(OEMNAME)"
    OwnerType="OEM"
    ReleaseType="Production"
    Platform="Common"
    Component="Custom"
    SubComponent="Cmd">
    <Components>
       <OSComponent>
	      <!-- Storing the Product Name in the registry in this package -->
          <RegKeys>
 		     <RegKey KeyName="$(hklm.software)\$(OEMNAME)\Product">
                <RegValue Name="StringValue" Value="$(PROD)" Type="REG_SZ"/>
             </RegKey>
         </RegKeys>		   
         <Files>
			<!-- 
			Source : OEM Customisation command is product specific, so copied from the Product directory
			Destination : C:\Windows\System32 which is the default. So destination not specified 
			-->
             <File Source="$(PRJDIR)\Updates\Update1\Custom.Cmd\oemcustomization.cmd" />
        </Files>
       </OSComponent>
    </Components>
 </Package>