<?xml version="1.0" encoding="utf-8"?>
<FeatureManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.microsoft.com/embedded/2004/10/ImageUpdate">
  <BasePackages>
     <PackageFile Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Microsoft-IoTUAP-RPi2-DeviceFM-Package.cab" FeatureIdentifierPackage="true"/>
   
     <PackageFile Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Microsoft-IoTUAP-RPi2-BCD-Package.cab" />

     <PackageFile Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Microsoft-IoTUAP-RPi2-Customization-Package.cab" /> 

  </BasePackages>

   <DeviceLayoutPackages>
    <PackageFile SOC="RPi2" Path="%PKGBLD_DIR%" Name="%OEM_NAME%.BSP.DeviceLayout.cab"/>
  </DeviceLayoutPackages>
  
  <Features>
    <MSFeatureGroups>
      <FeatureGroup Name="RPi2Base" PublishingFeatureGroup="true">
        <FeatureIDs>
          <FeatureID>BASE</FeatureID>	  
          <FeatureID>SOC_RPi2</FeatureID>		  
        </FeatureIDs>
       <SubGroups />
      </FeatureGroup>
    </MSFeatureGroups>  
    <Microsoft />
  </Features>

</FeatureManifest>
