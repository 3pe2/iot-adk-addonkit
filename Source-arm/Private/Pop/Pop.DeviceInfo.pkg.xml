<?xml version="1.0" encoding="utf-8"?>
<Package xmlns="urn:Microsoft.WindowsPhone/PackageSchema.v8.00"
         Owner="$(OEMNAME)"
         Component="POP"
         SubComponent="DeviceInfo"
         ReleaseType="Production"
         Platform="RPi2"
         OwnerType="OEM">
  <Components>
    <OSComponent>
      <RegKeys>
        <RegKey KeyName="$(HKLM.SYSTEM)\Platform\DeviceTargetingInfo">
          <RegValue Name="PhoneManufacturer"
                    Type="REG_SZ"
                    Value="IoTCoreOEM" />
          <RegValue Name="PhoneROMVersion"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneROMLanguage"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneMobileOperatorName"
                    Type="REG_SZ"
                    Value="000-88" />
          <RegValue Name="PhoneMobileOperatorDisplayName"
                    Type="REG_SZ"
                    Value="Operator" />
          <RegValue Name="PhoneModelName"
                    Type="REG_SZ"
                    Value="IoTCore_ARM" />
          <RegValue Name="PhoneManufacturerModelName"
                    Type="REG_SZ"
                    Value="IoTCore_ARM" />
          <RegValue Name="PhoneHardwareRevision"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneSOCVersion"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneFirmwareRevision"
                    Type="REG_SZ"
                    Value="0.0.0.0" />
          <RegValue Name="PhoneRadioHardwareRevision"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneBootLoaderVersion"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneSupportPhoneNumber"
                    Type="REG_SZ"
                    Value="0" />
          <RegValue Name="PhoneSupportLink"
                    Type="REG_SZ"
                    Value="http://portals/windowsphone8/client/wp8wiki/Pages/Devices.aspx" />
          <RegValue Name="PhoneOEMSupportLink"
                    Type="REG_SZ"
                    Value="http://portals/windowsphone8/client/wp8wiki/Pages/Devices.aspx" />
        </RegKey>
      </RegKeys>
    </OSComponent>
  </Components>
</Package>
