<Package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         Owner="$(OEMNAME)"
         Component="POP"
         SubComponent="DeviceTargetingInfo"
         OwnerType="OEM"
         ReleaseType="Production"
         Platform="MBM"
         xmlns="urn:Microsoft.WindowsPhone/PackageSchema.v8.00">
	<Components>
		<OSComponent>
			<RegKeys>
				<RegKey KeyName="$(hklm.system)\Platform\DeviceTargetingInfo">
					<RegValue 
                    Name="OneCoreManufacturer"
                    Type="REG_SZ"
                    Value="IoTCoreOEM"/>
					<RegValue 
                    Name="OneCoreManufacturerModelName"
                    Type="REG_SZ"
                    Value="IoTCore_x86"/>
					<RegValue 
                    Name="OneCoreOperatorName"
                    Type="REG_SZ"
                    Value="000-88"/>
					<RegValue 
                    Name="OneCoreFwV"
                    Type="REG_SZ"
                    Value="0.0.0.0"/>					
				</RegKey>
			</RegKeys>	
		</OSComponent>
	</Components>
</Package>

