<?xml version="1.0" encoding="utf-8"?>
 <Package xmlns="urn:Microsoft.WindowsPhone/PackageSchema.v8.00"
    Owner="$(OEMNAME)"
    OwnerType="OEM"
    ReleaseType="Production"
    Platform="Common"
    Component="Provisioning"
    SubComponent="Enroll">
    <Components>
       <OSComponent>
	   <Files>
		<!-- Manual Provisioning 
		     Packages are copied to OEM folder
			 Provisioning to be triggered in OEMCustomisation.cmd file by adding 
			 call provtool C:\OEMInstall\Enroll.ppkg
		-->
		<File Source="$(COMDIR)\Packages\Provisioning.Enroll\Enroll.ppkg"
			    DestinationDir="$(runtime.root)\OEMInstall\Provisioning" Name="Enroll.ppkg"/>
	   </Files>				
       </OSComponent>
    </Components>
 </Package>