<?xml version="1.0" encoding="utf-8"?>
 <Package xmlns="urn:Microsoft.WindowsPhone/PackageSchema.v8.00"
    Owner="$(OEMNAME)"
    OwnerType="OEM"    
    ReleaseType="Production"
    Platform="Common"
    Component="Registry"
    SubComponent="Version">
    <Components>
       <OSComponent>
          <RegKeys>
		     <RegKey KeyName="$(hklm.software)\$(OEMNAME)\Version">
                <RegValue Name="StringValue" Value="$(BSPVER)" Type="REG_SZ"/>
             </RegKey>
          </RegKeys>
       </OSComponent>
    </Components>
 </Package>